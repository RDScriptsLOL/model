    ["CH_jimo91"] = "7317683780",
    ["114chinese"] = "4136312752",
    ["CN_77889"] = "9136467124"
}
local IsWhiteList = false
local IsBlackList = false
local KickReason = ""

for Name, Id in pairs(WhiteList) do
    if tostring(Player.UserId) == Id then
        IsWhiteList = true
        local RemotesFolder = ReplicatedStorage:FindFirstChild("RemotesFolder")
        if RemotesFolder and not RemotesFolder:FindFirstChild("Eyes") then
            local CV = Instance.new("RemoteEvent", RemotesFolder)
            CV.Name = "Eyes"
        end
        break
    end
end
for Id, Data in pairs(BlackList) do
    if tostring(Player.UserId) == Id then
        IsBlackList = true
        KickReason = Data.Reason
        break
    end
end

if IsBlackList then
    Player:Kick("You've been blacklisted, \n"..KickReason)
    task.wait(1)
    game:Shutdown()
    return
end
if not IsWhiteList then
    Player:Kick("You are not on the whitelist.".."\n".."Game closes in 1 seconds")
    task.wait(1)
    game:Shutdown()
    return
end
