--在非规定时间要白名单的，给你加入黑名单
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Player = Players.LocalPlayer
local BlackList = {
    ["5208447980"] = {
        Name = "Thro354",
        Reason = "Reason: Support Fascism"
    }
}
local WhiteList = {
    ["Munciseek"] = "3296120580",
    ["麦兜"] = "3883436320",
    ["Wicked"] = "4912979218",
    ["FocusedLight"] = "4044271400",
